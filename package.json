{
  "name": "selectfromuser",
  "version": "1.0.4",
  "description": "A tool for admin/backoffice UI using YAML with SQL, RESTful API connectivity.",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "cd ui; npm run build;",
    "test": "ava tests/*.js",
    "start": "npm run api",
    "dev": "NODE_CONFIG_DIR=./config npm run api",
    "api": "PORT=8000 NODE_ENV=development nodemon ./bin/select --ignore ./tests --ignore ./ui --watch ./config/local.yml --watch ."
  },
  "bin": {
    "selectfromuser": "bin/select"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "debugs": "^1.0.11",
    "errorhandler": "^1.5.0",
    "excel-date-to-js": "^1.1.4",
    "express": "^4.16.4",
    "googleapis": "^92.0.0",
    "http-errors": "^1.7.1",
    "ioredis": "^4.28.3",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.3.0",
    "request-promise": "^4.2.6",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.14.1",
    "typeorm": "^0.2.12",
    "uuid": "^8.3.2",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "ava": "^1.2.0",
    "nodemon": "^1.18.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eces/select.git"
  },
